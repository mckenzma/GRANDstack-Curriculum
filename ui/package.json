    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.0",
    "prettier": "^1.15.2",
    "react": "^16.6.3",
    "react-apexcharts": "^1.3.3",
    "react-apollo": "^2.3.2",
    "react-dom": "^16.6.3",
    "react-scripts": "^2.1.1",
    "serve": "^10.1.1"
